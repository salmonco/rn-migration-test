{
  "extends": "@tsconfig/react-native/tsconfig.json",
  "include": ["**/*", "node_modules/nativewind"],
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "@assets/*": ["./src/assets/*"],
      "@apis/*": ["./src/apis/*"],
      "@components/*": ["./src/components/*"],
      "@screens/*": ["./src/screens/*"],
      "@stackNav/*": ["./src/stackNav/*"],
      "@tabNav/*": ["./src/tabNav/*"]
    }
  }
}
